digraph MooreMachine {
	Idle [label=Idle]
	1 [label=1]
	10 [label=10]
	101 [label=101]
	110 [label=110]
	1101 [label=1101]
	Idle -> Idle [label=0]
	Idle -> 1 [label=1]
	1 -> 1 [label=0]
	1 -> 10 [label=1]
	10 -> 10 [label=0]
	10 -> 101 [label=1]
	101 -> 101 [label=0]
	101 -> 110 [label=1]
	110 -> 110 [label=0]
	110 -> 1101 [label=1]
	1101 -> Idle [label=1]
	1101 -> Idle [label=0]
}
